{
  "resourceType": "Patient", // Tipo do recurso, sempre deve ser "Patient"
  "identifier": [
    // Identificadores do paciente
    {
      "system": "http://example.org/patient-ids", // Sistema de identificação
      "value": "12345" // Valor do identificador
    }
  ],
  "active": true, // Indica se o paciente está ativo (true) ou inativo (false)
  "name": [
    // Informações do nome do paciente
    {
      "family": "Sobrenome", // Sobrenome do paciente
      "given": ["Primeiro Nome"] // Primeiro nome do paciente (pode incluir mais de um)
    }
  ],
  "gender": "male", // Gênero do paciente (pode ser "male", "female", "other", "unknown", etc.)
  "birthDate": "1990-01-01", // Data de nascimento do paciente (formato: "YYYY-MM-DD")
  "address": [
    // Endereço do paciente
    {
      "use": "home", // Uso do endereço (pode ser "home", "work", "temp", etc.)
      "line": ["123 Rua Exemplo"], // Linha de endereço (pode incluir mais de uma)
      "city": "Cidade Exemplo", // Cidade
      "state": "Estado Exemplo", // Estado
      "postalCode": "12345" // Código postal
    }
  ],
  "telecom": [
    // Informações de contato do paciente
    {
      "system": "phone", // Sistema de contato (pode ser "phone", "email", "fax", etc.)
      "value": "123-456-7890", // Valor de contato
      "use": "home" // Uso do contato (pode ser "home", "work", "temp", etc.)
    },
    {
      "system": "email", // Sistema de contato (pode ser "phone", "email", "fax", etc.)
      "value": "exemplo@email.com", // Valor de contato
      "use": "home" // Uso do contato (pode ser "home", "work", "temp", etc.)
    }
  ],
  "maritalStatus": {
    // Estado civil do paciente
    "coding": [
      {
        "system": "http://hl7.org/fhir/v3/MaritalStatus", // Sistema de codificação
        "code": "M", // Código do estado civil
        "display": "Casado" // Descrição do estado civil
      }
    ]
  }
}
